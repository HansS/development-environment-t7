{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "rimraf build/* && cross-env BABEL_ENV=prod webpack",
    "start": "cross-env BABEL_ENV=dev webpack-dev-server --hot --inline",
    "test": "cross-env BABEL_ENV=test nyc ava",
    "lint": "standard source -verbose | snazzy"
  },
  "devDependencies": {
    "autoprefixer": "6.3.1",
    "ava": "0.11.0",
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-react-hmre": "1.0.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.7.2",
    "node-sass": "3.4.2",
    "nyc": "5.5.0",
    "postcss": "5.0.14",
    "postcss-loader": "0.8.0",
    "rimraf": "2.5.1",
    "sass-loader": "3.1.2",
    "snazzy": "2.0.1",
    "standard": "5.4.1",
    "style-loader": "0.13.0",
    "webpack": "1.12.12",
    "webpack-dev-server": "1.14.1"
  },
  "ava": {
    "files": [
      "source/**/*.test.js"
    ],
    "require": [
      "babel-core/register"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "test.js",
    "env": {
      "dev": {
        "presets": [
          "react-hmre"
        ]
      },
      "test": {
        "sourceMaps": "inline"
      }
    }
  },
  "dependencies": {
    "react": "0.14.7",
    "react-dom": "0.14.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t7/webpack-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/t7/webpack-starter/issues"
  },
  "homepage": "https://github.com/t7/webpack-starter#readme"
}
